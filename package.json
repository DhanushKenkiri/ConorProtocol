    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "web3modal": "^1.9.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@ethersproject/providers": "^5.8.0",
