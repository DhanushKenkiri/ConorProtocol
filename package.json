    "@vitejs/plugin-react-swc": "^3.9.0",
    "chai": "^4.3.7",
    "concurrently": "^9.1.2",
    "dotenv": "^16.3.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
