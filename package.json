{
  "name": "chronos-protocol",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile": "hardhat compile",
    "deploy": "hardhat run deployment/deploy.js",
    "test": "hardhat test",
    "blockchain": "hardhat node",
    "verify": "hardhat run deployment/verify.js",
    "start": "vite apps/web",
    "full-build": "npm run compile && npm run build",
    "type-check": "tsc -b",
    "deploy:base-sepolia": "node scripts/deploy-base-sepolia.js",
    "deploy:enhanced": "node scripts/enhanced-deploy.js",
