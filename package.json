    "launch": "node scripts/launch.js",
    "start:backend": "cd server && npm start",
    "start:all": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "dev:all": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "check:thirdweb": "node scripts/check-thirdweb-config.js",
    "check:alchemy": "node scripts/check-alchemy-config.js",
    "check:deployment": "node scripts/check-contract-status.js",
    "test:system": "node scripts/test-system.js",
    "test:alchemy-features": "node scripts/test-alchemy-features.js",
    "prepare:deploy": "node scripts/deploy.js",
    "setup": "node scripts/prepare-deployment.js",
    "docs": "echo \"\\033[1;34mAvailable documentation:\\033[0m\" && ls -la docs/ | grep \".md\" | awk '{print \"- \"$9}'",
    "demo": "node scripts/thirdweb-demo.js",
    "verify:blockscout": "node scripts/verify-blockscout.js"
  },
  "dependencies": {
