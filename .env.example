# Chronos Protocol .env.example - Copy this to .env and fill in your own values
# Don't commit the actual .env file to your repository!

# Private key for deploying contracts (without 0x prefix)
# Generate a new one for development purposes only, don't use your main wallet's key
PRIVATE_KEY=

# RPC endpoints
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# API keys for contract verification
BASESCAN_API_KEY=

# Factory contract address (will be set after deployment)
FACTORY_ADDRESS=
